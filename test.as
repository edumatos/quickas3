package {	import flash.display.MovieClip	public class test extends MovieClip{		function test(){						var s:ship = new ship()			s.x = 250			s.y = 250			$(s).enterFrame(				function(){					if($.isDown(38)){						var rot:Number = Math.PI/180*this.rotation						this.x += Math.cos(rot)*10						this.y += Math.sin(rot)*10					}					if($.isDown(40)){						var rot:Number = Math.PI/180*this.rotation						this.x -= Math.cos(rot)*10						this.y -= Math.sin(rot)*10					}					if($.isDown(39))						this.rotation+=5					if($.isDown(37))						this.rotation-=5				}			)			addChild(s)					}	}	}